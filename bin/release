#!/usr/bin/env bash
# bin/release <build-dir>

BUILD_DIR=$1

cat <<EOF
---
config_vars:
  PATH: /app/.jdk/bin:/usr/local/bin:/usr/bin:/bin
  JAVA_OPTS: -Xmx256m -Xms256-Xss512k -XX:+UseCompressedOops -javaagent:newrelic/newrelic.jar
  MAVEN_OPTS: -Xmx384m -Xss512k -XX:+UseCompressedOops 
addons:
  heroku-postgresql:dev
EOF
